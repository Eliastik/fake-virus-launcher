{
  "name": "fake-virus-pack",
  "version": "0.0.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "ng build --base-href ./ && electron .",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "localize": "ng extract-i18n --format=json",
    "postinstall": "patch-package",
    "package": "ng build --base-href ./ && electron-builder --win portable",
    "package:32": "ng build --base-href ./ && electron-builder --win portable --ia32",
    "package-linux": "ng build --base-href ./ && electron-builder --linux appImage"
  },
  "private": true,
  "devDependencies": {
    "ngx-electron": "^2.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0",
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@angular-devkit/build-angular": "^15.2.0",
    "@angular/cli": "~15.2.0",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "electron": "^23.1.1",
    "electron-builder": "^23.6.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "patch-package": "^6.5.1",
    "typescript": "~4.9.4"
  },
  "build": {
    "productName": "Virus Launcher by Eliastik's Softs",
    "appId": "eliastik.virusLauncher",
    "win": {
      "target": [
        "portable"
      ]
    },
    "portable": {
      "artifactName": "virus-launcher.exe"
    },
    "directories": {
      "output": "electron/output"
    },
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes,.angular,.vscode}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!**/src/*",
      "!**/patches/*",
      "!**/electron/*",
      "!**/angular.json",
      "!**/package-lock.json",
      "!**/package.json",
      "!**/README.md",
      "!**/tsconfig.json",
      "!**/tsconfig.spec.json",
      "!**/tsconfig.app.json"
    ]
  }
}
