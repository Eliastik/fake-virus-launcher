{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "eliastik.virusLauncher",
  "version": "2.1.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/build/fake-virus-pack",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "storage.*",
    "window.*"
  ],
  "globalVariables": {},
  "modes": {
    "window": {
      "title": "Virus Launcher",
      "width": 800,
      "height": 600,
      "minWidth": 800,
      "minHeight": 600,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false,
      "useSavedState": false
    },
    "browser": {
      "globalVariables": {},
      "nativeBlockList": [
        "filesystem.*"
      ]
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "nativeBlockList": [
        "filesystem.*",
        "os.*"
      ]
    }
  },
  "cli": {
    "binaryName": "virus-launcher",
    "resourcesPath": "/resources/build/fake-virus-pack/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "5.0.0",
    "clientVersion": "5.0.0"
  }
}
